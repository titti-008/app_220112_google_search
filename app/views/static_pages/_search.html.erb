<h2 class="primary"><%= link_to 'Geegle検索', root_path %></h2>

<div class="col-md-8" >
    <%= form_with( url: search_path, method: :get,class:"form-inline my-2") do%>
      <%= text_field_tag :search, params[:search], placeholder:"文字検索", class:"col-md-8 form-control mr-sm-2" %>
      <%= submit_tag "検索",name:nil, class:"col-3 btn btn-outline-success my-2 my-sm-0" %>

    <% end %>
</div>
